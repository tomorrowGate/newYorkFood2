/* find/components/dialog.wxss */
.dialog{
  position: fixed;
  width: 100vw;
  height: 100vh;
  z-index: 2
}
.dialog-mask{
  position: fixed;
  top:0;
  width: 100vw;
  height: 100vh;
  background: rgba(0,0,0,0.5);
  z-index: 3
}
.dialog-main{
  position: fixed;
  width: 634rpx;
  height: 568rpx;
  background: #fff;
  border-radius: 16rpx;
  top:50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 4;
}