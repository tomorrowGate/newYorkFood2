<!--classify/page/classify_searchbody.wxml-->
<classifyBody renderdata="{{renderdata}}" bind:fromChild="fromChild">
  <view class="nodata-index" wx:if="{{renderdata.length<=0}}">
   <view class="nodata-imgbox">
      <image src="{{imgsrc}}"></image>
    </view>
    <text>{{nodata}}</text>
  </view>
</classifyBody>