.food-list .content .con-item .con-center .con-cen-foot .yellow {
  color: #e8af62;
  border: 1px solid #e8af62;
}
.food-list .content .con-item .con-right {
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: space-between; */
  padding-top: 5.66vw ;
  box-sizing: border-box;
}
.food-list .content .con-item .con-right .con-rig-tag {
  padding: 0 1.66666667vw;
  color: #ffac41;
  border: 1px solid #ffac41;
  border-radius:6rpx;
  margin-bottom: 10rpx;
}
.food-list .content .con-item .con-right .con-rig-meter {
  /* margin-bottom: 2.77777778vw; */
}
.food-list .content .con-item .con-right .con-rig-meter .icondingwei {
  color: #e8af62;
  font-size: 26rpx;
}

.con-cen-title .collection{
   font-size: 24rpx;
   background: #de3e22;
   color: #fff;
   padding: 0 16rpx;
   border-radius: 18rpx;
   margin-left: 10rpx;
   white-space: nowrap;
}

.clear-collect{
  position: absolute;
  display: inline-block;
  width: 200rpx;
  height: 100%;
  /* line-height: 300rpx; */
  margin-left:6rpx;
  background: #de3e22;
  color: #fff;
  text-align: center;
}
.clear-collect .text{
  position: absolute;
  width: 200rpx;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  margin-top: -20rpx;
  font-size: 28rpx;
  word-wrap: break-word;
}
