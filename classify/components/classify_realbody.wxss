/* classify/components/classify_realbody.wxss */
@import "/app.wxss";
.mapFood {
  background: #fff;
  /* margin-top: 20rpx; */
}
.view {
  position: relative;
  width: 100%;
  white-space: nowrap;
  /* padding-bottom: 20rpx; */
  padding:20rpx 0rpx 0rpx;
}
.real-scroll{
  height: auto;
  box-sizing: border-box;
  display: inline-flex;
  /* font-size:0px; */
}
.food-list {
  position: relative;
  display: inline-block;
  padding: 0 3.7037037vw;
  font-size: 3.7037037vw;
  width: 750rpx;
  box-sizing: border-box;
  /* border:1px solid green; */
}
.food-list .head {
  display: flex;
  align-items: center;
  width: 100%;
  height: 6.94444444vw;
  border-bottom: 1px solid #dedede;
}
.food-list .head .head-text {
  text-indent: 1.85185185vw;
}
.food-list .head .iconfont {
  font-size: 3.7037037vw;
}
.food-list .content .out {
  display: flex;
  flex:1;
  justify-content: space-around;
  border-bottom: 1px solid #dedede;
  padding-bottom: 16rpx;
}
.food-list .content .con-item {
  display: flex;
  padding-top: 20rpx;
}
.food-list .content .con-item .con-left {
  position: relative;
  width: 18.51851852vw;
  margin-right: 2.22222222vw;
}
.food-list .content .con-item .con-left .imgbox {
  width: 18.51851852vw;
  height: 18.51851852vw;
}
.food-list .content .con-item .con-left .con-say {
  position: absolute;
  top: 18.51851852vw;
  width: 18.51851852vw;
  height: 4.62962963vw;
  line-height: 4.62962963vw;
  text-align: center;
  background: #ffac41;
  color: #fff;
}
 .food-list .content .con-item .con-center {
  display: flex;
  flex-direction: column;
  justify-content: space-around;

  width: 52.77777778vw;
  height: 100%;
  white-space: normal;
  /* border:1px solid red; */
}
.food-list .content .con-item .con-center .con-cen-title {
  display: flex;
  align-items: center;
  font-weight: bold;
  font-size: 4.62962963vw;
  flex-wrap: wrap;
  /* height: 4.62962963vw;
  line-height: 4.62962963vw; */
  margin-bottom: 0.92592593vw;
  position: relative;
  top: -6rpx;
}
.food-list .content .con-item .con-center .con-cen-tags {
  display: flex;
  position:relative;
  top: -6rpx;
  flex-wrap:wrap;
}
 .food-list .content .con-item .con-center .con-cen-tags .con-cen-tag {
  height: 4.44444444vw;
  line-height: 4.44444444vw;
  padding: 0 1.66666667vw;
  background: #ffe1ec;
  color: #de3e22;
  margin-right: 1.85185185vw;
  border-radius: 6rpx;
  margin-top: 6rpx;
  white-space: nowrap;
}
.food-list .content .con-item .con-center .con-cen-palce {
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
color: #666;
width: 100%;
margin: 0.92592593vw 0 0;
padding: 0px;
/* line-height: 26rpx; */

}
.food-list .content .con-item .con-center .con-cen-foot {
  display: flex;
  margin-top: 15rpx;
  flex-wrap:wrap;
} 

