/* classify/components/classify_realbody.wxss */
@import "/app.wxss";
.mapFood {
  background: #fff;
}
.view {
  width: 100%;
  height: 286rpx;
  white-space: nowrap;
}
.food-list {
  position: relative;
  display: inline-block;
  padding: 0 3.7037037vw;
  font-size: 3.7037037vw;
  width: 750rpx;
  box-sizing: border-box;
}
.food-list .head {
  display: flex;
  align-items: center;
  width: 100%;
  height: 6.94444444vw;
  border-bottom: 1px solid #dedede;
}
.food-list .head .head-text {
  text-indent: 1.85185185vw;
}
.food-list .head .iconfont {
  font-size: 3.7037037vw;
}
.food-list .content .out {
  display: flex;
  flex:1;
  justify-content: space-around;
  border-bottom: 1px solid #dedede;
  padding-bottom: 16rpx;
}
.food-list .content .con-item {
  display: flex;
  height: 33.33333333vw;
 /*  padding: 3.24074074vw 0; */
  /* box-sizing: border-box; */
}
.food-list .content .con-item .con-left {
  position: relative;
  width: 18.51851852vw;
  height: 33.33333333vw;
  margin-right: 2.22222222vw;
}
.food-list .content .con-item .con-left .imgbox {
  width: 18.51851852vw;
  height: 18.51851852vw;
  /* background: #f60; */
}
.food-list .content .con-item .con-left .con-say {
  position: absolute;
  top: 18.51851852vw;
  width: 18.51851852vw;
  height: 4.62962963vw;
  line-height: 4.62962963vw;
  text-align: center;
  background: #ffac41;
  color: #fff;
}
.food-list .content .con-item .con-center {
  /* display: flex; */
  display: flex;
  flex-direction: column;
  justify-content: space-around;

  width: 52.77777778vw;
  height: 100%;
  white-space: normal;
}
.food-list .content .con-item .con-center .con-cen-title {
  font-weight: bold;
  font-size: 4.62962963vw;
  margin-bottom: 0.92592593vw;
}
.food-list .content .con-item .con-center .con-cen-tags {
  display: flex;
}
.food-list .content .con-item .con-center .con-cen-tags .con-cen-tag {
  height: 4.44444444vw;
  line-height: 4.44444444vw;
  padding: 0 1.66666667vw;
  background: #ffe1ec;
  color: #de3e22;
  margin-right: 1.85185185vw;
  border-radius: 6rpx;
}
.food-list .content .con-item .con-center .con-cen-palce {
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
  color: #666;
  width: 100%;
  margin: 0.92592593vw 0;
}
.food-list .content .con-item .con-center .con-cen-foot {
  display: flex;
}
.food-list .content .con-item .con-center .con-cen-foot .con-cen-foottag {
  padding: 0 1.66666667vw;
  margin-right: 1.85185185vw;
}
.food-list .content .con-item .con-center .con-cen-foot .red {
  color: #de3e22;
  border: 1px solid #eb877f;
  border-radius: 6rpx;
  /* max-width: 52vw;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
}
.food-list .content .con-item .con-center .con-cen-foot .yellow {
  color: #e8af62;
  border: 1px solid #e8af62;
}
.food-list .content .con-item .con-right {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  padding: 3.33333333vw 0;
  box-sizing: border-box;
}
.food-list .content .con-item .con-right .con-rig-tag {
  padding: 0 1.66666667vw;
  color: #ffac41;
  border: 1px solid #ffac41;
  border-radius:6rpx;
}
.food-list .content .con-item .con-right .con-rig-meter {
  margin-bottom: 2.77777778vw;
}
.food-list .content .con-item .con-right .con-rig-meter .icondingwei {
  color: #e8af62;
  font-size: 26rpx;
}

.con-cen-title .collection{
   font-size: 24rpx;
   background: #de3e22;
   color: #fff;
   padding: 0 16rpx;
   border-radius: 18rpx;
   white-space: nowrap;
}

.clear-collect{
  position: relative;
  display: inline-block;
  width: 200rpx;
  height: 276rpx;
  line-height: 300rpx;
  margin-left:6rpx;
  background: #de3e22;
  color: #fff;
  text-align: center;
}
.clear-collect .text{
  width: 200rpx;
  transform: translate(0rpx,-20rpx);
  font-size: 28rpx;
  word-wrap: break-word;
}
