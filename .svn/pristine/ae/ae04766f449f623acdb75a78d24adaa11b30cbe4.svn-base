/* information/personal.wxss */
/* 个人信息 */

.row{
height:100rpx;
border-bottom: 2px solid #f2f2f2;
display: flex;
justify-content: space-between;
align-items: center;
margin: 0 30rpx;
letter-spacing: 2rpx;

}
.row:first-child{
  margin-top: 38rpx;
  
}
.last{
  border-bottom: none;
}
.label{
  line-height: 120rpx;
  font-size: 36rpx;
}
.row-text-area{
  color: #8c8c8c;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.img{
  height: 100rpx;
  width: 100rpx;
  border: 1px solid #f2f2f2;
  border-radius: 50%;
}
.head{
  display: flex;
  height: 100rpx;
  width: 100rpx;
  justify-content: space-between;
  align-items:center;
  color: #8c8c8c;
}
.name{
  display: flex;
  justify-content: space-between;
  align-items:center;
  /* width: 250rpx; */
  color: #8c8c8c;
 }
.phone{
  display: flex;
  justify-content: space-between;
  align-items:center;
  /* width: 180rpx; */
  color: #8c8c8c;
}
.sex{
  display: flex;
  justify-content: space-between;
  align-items:center;
  /* width: 110rpx; */
  color: #8c8c8c;
}
.birthday{
  display: flex;
  justify-content: space-between;
  align-items:center;
  /* width: 290rpx; */
  color: #8c8c8c;
}
.row-right{
  display: flex;
  justify-content: space-between;
  align-items:center;
  color: #8c8c8c;
}
.row-text{
  margin-right: 20rpx;
}
.iconyoujiantou{
  color: #8c8c8c;
}
.personality{
  padding: 20rpx 40rpx;
  letter-spacing: 5rpx;
  font-size: 32rpx;
}


