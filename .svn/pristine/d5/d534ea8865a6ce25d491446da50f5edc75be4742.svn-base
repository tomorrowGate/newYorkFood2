<!--my/page/my_index.wxml-->
<view class="chang-lang" wx:if="{{false}}">
  <view class="changeLan-ul {{isLanActive?'langs-active':''}}">
      <view class="lang-item" bindtap="showLang">{{selectItem}}</view>
      <view class="lang-item" bindtap="hideLang" data-name="中文" data-value="{{0}}">中文</view>
      <view class="lang-item" bindtap="hideLang" data-name="English" data-value="{{1}}">English</view>
  </view>
</view>
<myHead></myHead>
<myCard></myCard>
<mySer></mySer>