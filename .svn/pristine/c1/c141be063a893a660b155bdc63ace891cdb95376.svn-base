<!--member/page/menber_index.wxml-->
<!--pages/vipmember/vip.wxml-->
<view class="content">
    <!--pics文件夹下的background.jpg文件-->
    <image class='background' src="/src/img/customBg.jpg" mode="aspectFill"></image>
    <!--页面其它部分-->
</view>
<!-- 图片部分 -->
<view class="image">
  <view class="img imgbox"> <image src="/src/img/cusBlock.png" ></image></view>
  <image src="/src/img/memberlogo.png" class="logo"> </image>
  <view class="content1">
      <view class="title">{{language.vipcards[0]}}</view>
      <view class="title_name">{{language.vipcards[1]}}</view>
      <view class="line"></view>
      <view class="icon"><i class="iconfont iconjiantou9"></i></view>
  </view>
</view>
    <!-- 底部 -->
<view class="last-one">
  <view class="top">
    <view class="top-imgbox"><image src="/src/img/trangle.png"></image></view>
    <view class="title">{{language.vipcards[2]}}</view>
  </view>
  <view class="bottom">
    <view class="item">
      <view>{{language.vipcards[3]}}</view>
      <view>{{language.vipcards[4]}}</view>
    </view>
    <view class="item">
      <view>{{language.vipcards[5]}}</view>
      <view>{{language.vipcards[6]}}</view>
    </view>
    <view class="item">
      <view>{{language.vipcards[7]}}</view>
      <view>{{language.vipcards[8]}}</view>
    </view>
  </view>
</view>

<view class="last-two">
  <view class="top">
    <view class="top-imgbox"><image src="/src/img/trangle.png"></image></view>
    <view class="title">{{language.vipcards[9]}}</view>
  </view>
  <view class="bottom">
    <view class="item">
      <view>{{language.vipcards[10]}}</view>
      <view>{{language.vipcards[11]}}</view>
    </view>
    <view class="item">
      <view>{{language.vipcards[12]}}</view>
      <view>{{language.vipcards[13]}}</view>
    </view>
    <view class="item">
      <view>{{language.vipcards[14]}}</view>
      <view>{{language.vipcards[15]}}</view>
    </view>
  </view>
</view>


