/* find/components/find_video.wxss */
@import "/app.wxss";
.find-video{
  padding: 0 23rpx 30rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  background: #fff;
}
.find-video-title{
  display: flex;
  align-items: center;
  height: 104rpx;
  line-height: 104rpx;
  font-weight: bold;
}
.find-video-title::before{
  display: block;
  content: "";
  height: 30%;
  border:2rpx solid #ffac41;
  margin-right: 20rpx;
}
.find-video-items{
  height: 266rpx;
}
.find-video-out{
  white-space: nowrap;
}
.find-video-item{
  display: inline-block;
  width: 300rpx;
  height: 266rpx;
  margin-left: 20rpx;
}
.find-video-item video{
  width: 100%;
  height: 100%;
}