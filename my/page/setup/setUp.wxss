/* setUp/setUp.wxss */
/* 设置 */
.head{
  height: 420rpx;
  box-sizing: border-box;
  padding: 46rpx 36%;
  border-bottom: 10px solid #f2f2f2;
}
.img{
  border: 1px solid #f2f2f2;
  border-radius: 50%;
  height: 200rpx;
  width: 200rpx;
  margin-bottom: 20rpx;
  }
.call{
  font-size: 34rpx;
}


.option{
  height:100rpx;
border-bottom: 2px solid #f2f2f2;
margin: 0 25rpx;
line-height: 100rpx;
font-size: 32rpx;
color: #959595;

}
.option:last-child{
  display: flex;
  justify-content: space-between;
  border-bottom: none;
}



